{
  "name": "web-element-popover",
  "version": "1.0.7",
  "description": "The auto popover solution for the web",
  "type": "module",
  "main": "./dist/lib.umd.js",
  "module": "./dist/lib.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib.es",
      "require": "./dist/lib.umd"
    },
    "./README.md": "./README.md",
    "./style.css": "./dist/index.css"
  },
  "keywords": ["typescript", "javascript", "popover", "component", "ui", "frontend"],
  "scripts": {
    "demo": "cross-env BASE_ENV=development vite",
    "build:clean": "rimraf ./dist",
    "build": "cross-env BASE_ENV=production vite build",
    "test": "vitest",
    "publish": "npm publish --registry https://registry.npmjs.org"
  },
  "author": "senmacn",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-plugin-prettier": "^5.2.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "tippy.js": "^6.3.7"
  }
}
